* Measure JNW_GR03_TB
.control

load {cicname}.raw

echo "MEAS_START"

#ifdef Temperaturesweep
foreach vtemp {temp_sweep}
#else
*foreach vtemp 27
foreach vtemp {temp_sweep}
#endif
    echo "LINE_SHIFT"
    echo "LINE_SHIFT"
    set fend = .raw
    load {cicname}_$vtemp$fend

    meas tran i_temp$vtemp find i(VSENS) at=100n

end

echo "MEAS_END"
.endc
